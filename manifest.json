{
    "manifest_version": 2,
    "name": "Simple Blocker",
    "version": "1.0.0",
    "description": "Blocks harmful websites by keywords.",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "tabs",
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "128": "icon.png"
    }
  }